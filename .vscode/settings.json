{
  "editor.formatOnSave": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "files.eol": "\n",
  "cSpell.words": [
    "ITASK"
  ]
}
